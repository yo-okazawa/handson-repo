#!/bin/bash
 
expect -c '
 
spawn /var/f-secure-linux-security-9.20.2520/f-secure-linux-security-9.20.2520 --command-line-only
set timeout 60
expect {
  -regexp "Press enter to view license agreement." {
    exp_send "\n" 
  }
}
expect {
  -regexp "--More--*" {
    exp_send "q" 
  }
}
expect {
  -regexp "Do you accept this license agreement?" {
    exp_send "yes\n" 
  }
}

expect {
  -regexp "keycode:" {
    exp_send "\n" 
    exp_continue
  }
}
'